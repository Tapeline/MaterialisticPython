{
  "manifest_version": 3,
  "name": "Materialistic python",
  "description": "Enhance docs.python.org UI",
  "version": "1.0",
  "action": {
    "default_popup": "info.html",
    "default_icon": "icon.png"
  },
  "permissions": ["storage", "activeTab", "declarativeContent", "scripting"],
  "content_scripts": [
    {
      "js": ["lib/jquery.js", "lib/codemirror.min.js", "lib/mode/clike.js",
        "scripts/content.js", "scripts/switchTheme.js", "lib/materialize.min.js"],
      "css": ["lib/materialize.min.css", "main.css", "lib/codemirror.min.css"],
      "matches": [
        "https://docs.python.org/*"
      ]
    }
  ]
}
